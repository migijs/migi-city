{
  "name": "migi-city",
  "version": "0.1.0",
  "description": "A city WebComponent of migi",
  "maintainers": [
    {
      "name": "army8735",
      "email": "army8735@qq.com"
    }
  ],
  "scripts": {
    "test": "make test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/migijs/migi-city.git"
  },
  "keywords": [
    "migi",
    "city",
    "WebComponent"
  ],
  "author": "army8735",
  "license": "MIT",
  "dependencies": {
    "migi": "~0.6.1",
    "es5-shim": "*",
    "es6-shim": "~0.32.0"
  },
  "devDependencies": {
    "nightwatch": "~0.8.6",
    "migi-loader": "~0.6.0",
    "more-loader": "~0.1.2"
  },
  "main": "./index",
  "engines": {
    "node": ">= 0.4.2"
  },
  "spm": {
    "main": "./MigiCity.jsx",
    "output": [
      "./src/MigiCity.jsx",
      "./src/style.cssx"
    ],
    "build": {
      "autoprefixer": true,
      "loader": {
        ".jsx": "migi-loader?es5=true&lie=true",
        ".cssx": "migi-loader?css=true"
      }
    }
  },
  "readmeFilename": "README.md"
}
